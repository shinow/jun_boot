       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="higherOrderBeanPostProcessor" class="com.alipay.sofa.runtime.integration.postprocessor.HigherOrderBeanPostProcessor"/>

    <bean id="lowOrderBeanPostProcessor" class="com.alipay.sofa.runtime.integration.postprocessor.LowOrderBeanPostProcessor"/>

    <bean id="beanPostProcessorTestBean" class="com.alipay.sofa.runtime.integration.postprocessor.BeanPostProcessorTestBean"/>
</beans>